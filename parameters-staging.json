{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "uri": {
        "value": "https://raw.githubusercontent.com/pragadeshwara/new/main/"
      },
      "environment": {
        "value": "staging"
      },
      "resourceGroupName": {
        "value": "ADinfrastructure"
      },
      "location": {
        "value": "UKsouth"
      },
      "Nsg-rule": {
        "value": {
          "protocol": "Tcp",
          "sourcePortRange": "*",
          "destinationPortRange": "22",
          "sourceAddressPrefix": "*",
          "destinationAddressPrefix": "*",
          "access": "Allow",
          "direction": "Inbound",
          "priority": 100,
          "RuleName": "Allow_SSH"
        }
      },
      "address-prefix": {
        "value": "10.0.0.0/16"
      },
      "subnet1-address-prefix": {
        "value": "10.0.1.0/24"
      },
      "storage-kind":{
        "value": "StorageV2"
      },
      "storage-sku-name":{
        "value": "Standard_LRS"
      },
      "serverName": {
        "value": "myPostgreSQLServer"
      },
      "administratorLogin": {
        "value": "adminUser"
      },
      "administratorPassword": {
        "value": "sqladminpassword"
      },
      "databaseName": {
        "value": "myDatabase"
      }
    }
}